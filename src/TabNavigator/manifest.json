{
  "displayName": "Tab Bar",
  "defaultWidth": 375,
  "defaultHeight": 56,
  "snappingRules": {
    "snap": true,
    "snapType": "screenEdge",
    "lock": {
      "left": 0,
      "right": 0,
      "bottom": 0
    }
  },
  "props": [
    {
      "name": "backgroundColor",
      "displayName": "Fill Color",
      "type": "color",
      "default": "#fff"
    },
    {
      "name": "activeColor",
      "displayName": "Active Color",
      "type": "color",
      "default": "#6200ee"
    },
    {
      "name": "inactiveColor",
      "editable": "Inactive Color",
      "type": "color",
      "default": "#bbb"
    },
    {
      "name": "positioning",
      "editable": false,
      "type": "text",
      "default": "fixedTop"
    }
  ],
  "childComponents": [
    {
      "name": "tab0",
      "displayName": "First Item",
      "actions": true,
      "props": [
        {
          "name": "icon",
          "displayName": "Icon",
          "type": "icon",
          "default": "home"
        },
        {
          "name": "label",
          "displayName": "Text",
          "type": "text",
          "default": "Home"
        }
      ]
    },
    {
      "name": "tab1",
      "displayName": "Second Item",
      "actions": true,
      "props": [
        {
          "name": "icon",
          "displayName": "Icon",
          "type": "icon",
          "default": "search"
        },
        {
          "name": "enabled",
          "type": "boolean",
          "default": true
        },
        {
          "name": "label",
          "displayName": "Text",
          "type": "text",
          "default": "Search"
        }
      ]
    },
    {
      "name": "tab2",
      "displayName": "Third Item",
      "actions": true,
      "props": [
        {
          "name": "icon",
          "displayName": "Icon",
          "type": "icon",
          "default": "people"
        },
        {
          "name": "enabled",
          "type": "boolean",
          "default": true
        },
        {
          "name": "label",
          "displayName": "Text",
          "type": "text",
          "default": "Team"
        }
      ]
    },
    {
      "name": "tab3",
      "displayName": "Fourth Item",
      "actions": true,
      "props": [
        {
          "name": "icon",
          "displayName": "Icon",
          "type": "icon",
          "default": "account-circle"
        },
        {
          "name": "enabled",
          "type": "boolean",
          "default": true
        },
        {
          "name": "label",
          "displayName": "Text",
          "type": "text",
          "default": "Profile"
        }
      ]
    },
    {
      "name": "tab4",
      "displayName": "Fifth Item",
      "actions": true,
      "props": [
        {
          "name": "icon",
          "displayName": "Icon",
          "type": "icon",
          "default": "more-horiz"
        },
        {
          "name": "enabled",
          "type": "boolean",
          "default": true
        },
        {
          "name": "label",
          "displayName": "Text",
          "type": "text",
          "default": "More"
        }
      ]
    }
  ]
}
