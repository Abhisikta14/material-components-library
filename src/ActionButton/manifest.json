{
  "displayName": "Action Button",
  "icon": "icon.png",
  "defaultWidth": 56,
  "defaultHeight": 56,
  "component": "./ActionButton.js",
  "resizeX": false,
  "resizeY": false,
  "props": [
    {
      "name": "icon",
      "displayName": "Icon",
      "type": "icon",
      "default": "add"
    },
    {
      "name": "action",
      "type": "action",
      "displayName": "On Press"
    },
    {
      "name": "backgroundColor",
      "displayName": "Fill Color",
      "type": "color",
      "default": "#6200ee"
    },
    {
      "name": "color",
      "displayName": "Icon Color",
      "type": "color",
      "default": "#fff"
    },
    {
      "name": "positioning",
      "editable": false,
      "type": "text",
      "default": "fixedBottom"
    }
  ]
}

