{
  "displayName": "Action Button",
  "icon": "icon.png",
  "defaultWidth": 48,
  "defaultHeight": 48,
  "component": "./ActionButton.js",
  "resizeX": false,
  "resizeY": false,
  "props": [
    {
      "name": "icon",
      "displayName": "Icon",
      "type": "icon",
      "default": "add"
    },
    {
      "name": "action",
      "type": "action",
      "displayName": "Click Actions"
    },
    {
      "name": "backgroundColor",
      "displayName": "Fill Color",
      "type": "color",
      "default": "@secondary"
    },
    {
      "name": "color",
      "displayName": "Icon Color",
      "type": "color",
      "default": "@contrast:backgroundColor"
    },
    {
      "name": "positioning",
      "editable": false,
      "type": "text",
      "default": "fixedBottom"
    },
    {
      "name": "text",
      "displayName": "text",
      "type": "text",
      "styles": {
        "fontFamily": "@body",
        "fontWeight": "600"
      }
    }
  ]
}
