{
  "displayName": "Toggle",
  "icon": "icon.png",
  "defaultWidth": 24,
  "defaultHeight": 24,
  "component": "./index.js",
  "resizeX": false,
  "resizeY": false,
  "props": [
    {
      "name": "input",
      "displayName": "What does this toggle?",
      "type": "boolean",
      "role": "autosaveInput"
    },
    {
      "name": "activeIcon",
      "displayName": "Active Icon",
      "type": "icon",
      "default": "check-box"
    },
    {
      "name": "activeColor",
      "displayName": "Active Color",
      "type": "color",
      "default": "@primary"
    },
    {
      "name": "inactiveIcon",
      "displayName": "Inactive Icon",
      "type": "icon",
      "default": "check-box-outline-blank"
    },
    {
      "name": "inactiveColor",
      "displayName": "Inactive Color",
      "type": "color",
      "default": "#bbb"
    },
    {
      "name": "clickActions",
      "displayName": "Click Actions",
      "type": "action" 
    }
  ]
}

