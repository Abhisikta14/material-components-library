{
  "displayName": "Toggle",
  "icon": "icon.png",
  "defaultWidth": 48,
  "defaultHeight": 48,
  "component": "./index.js",
  "resizeX": false,
  "resizeY": false,
  "props": [
    {
      "name": "activeIcon",
      "displayName": "Active Icon",
      "type": "icon",
      "default": "check-box"
    },
    {
      "name": "inactiveIcon",
      "displayName": "Inactive Icon",
      "type": "icon",
      "default": "check-box-outline-blank"
    },
    {
      "name": "activeColor",
      "displayName": "Active Color",
      "type": "color",
      "default": "@primary"
    },
    {
      "name": "inactiveColor",
      "displayName": "Inactive Color",
      "type": "color",
      "default": "#bbb"
    },
    {
      "name": "value",
      "displayName": "Value",
      "type": "boolean",
      "default": true,
      "role": "formValue"
    },
    {
      "name": "onChange",
      "type": "action",
      "displayName": "On Change",
      "role": "formChangeHandler",
      "reference": "value"
    }
  ]
}

